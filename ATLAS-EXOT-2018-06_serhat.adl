#info analysis
#  title "Search for new phenomena in events with an energetic jet and missing transverse momentum in âˆšp p collisions at s = 13 TeV with the ATLAS detector"
#  experiment ATLAS
#  id EXOT-2018-06

# OBJECT DEFINITIONS

object jets
  take Jet
  select pT(Jet) > 20
  select abs(eta(Jet)) < 2.8
  
object cjets
  take jets
  select pT(jets) < 120
  select abs(eta(jets)) < 2.5
  
object btagjets
  take jets
  select pT(jets) > 30
  select abs(eta(jets)) < 2.5
  
object electrons
  take Ele
  select pT(Ele) > 7
  select abs(eta(Ele)) <2.47
  
object muons
  take Muo
  select pT(Muo) > 7
  select abs(eta(Muo)) < 2.5
  
object tau
  take jets
  select pT(jets) > 10
  select abs(eta(jets)) < 2.5
  
object tauloose
  take tau
  select pT(tau) > 20
  select abs(eta(tau)) < 2.5
  reject abs(eta(tau)) [] 1.37 1.52
  reject dR(electrons, tau) <= 0.2
  reject dR(muons, tau) <= 0.2
  reject dR(jets, tau) == 0.2
  
  
object photons
  take Pho
  select pT(Pho) > 10
  select abs(eta(Pho)) < 2.37
  #select dR(, electron) < 0.4
  
 
#EVENT SELECTION

region EM0
  select ALL
  select MET [] 200 250
  select Size(jets) >=1
  select pT(jets[0]) > 150 && abs(eta(jets[0])) < 2.4
  select pT(jets[1:3]) > 30 && abs(eta(jets[1:3]))< 2.8
  
